<div class="row sign-up-form">
    <div class="col s6 m6">
        <div class="card blue-gray darken-1">
            <div class="card-content ">
                <span class="card-title"><%= @student.name%></span>
                <p><b>Email: </b><%= @student.email %></p>
                <p>Course Enrollments: None</p>
            </div>
        </div>
        <% if current_user == @student %>
            <%= link_to 'Edit Your Profile<i class="material-icons right">create</i>'.html_safe, edit_student_path(@student), class: "waves-effect waves-light btn" %>
        <% end %>
    </div>
</div>