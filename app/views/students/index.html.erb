<div class="row">
    <div class="col s12">
        <h3 class="center-align">List of Students</h3>
    </div>
</div>
<div class="row">
    <% @students.each do |student| %>
    <div class="col s6 m6">
        <div class="card blue-gray darken-1">
            <div class="card-content ">
                <span class="card-title"><%= student.name%></span>
                <p><b>Email: </b><%= student.email %></p>
                <p>Course Enrollments: None</p>
            </div>
            <div class="card-action">
                <%= link_to 'View Profile', student_path(student) %>
            </div>
        </div>
    </div>
    <% end %>
</div>